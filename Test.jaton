class Main {
    public static void main() {
        double i;
        double j;
        double k;

        i = 2;
        while(i) {
            j = 2;
            while(j) {
                k = 2;
                while(k) {
                    println(i + " " + j + " " + k);
                    k = k - 1;
                }
                j = j - 1;
            }
            i = i - 1;
        }
    }
}
